{% if user_list %}
        <table class="table">
            <thead>
            <tr>

                <th scope="col">Date du jour</th>
                <th scope="col">Matinée</th>
                <th scope="col">Après-midi</th>
                <th scope="col">Prénom</th>
                <th scope="col">Nom</th>

            </tr>
            </thead>
            <tbody>
            {% for user in user_list %}
                <tr>

                    <td>{{ "now"|date("m/d/Y H:i:s")|format_datetime('full', 'short', locale='fr') }}</td>
                    <td>{{ has_presence(user, half_journey_morning) }}</td>
                    <td>{{ has_presence(user, half_journey_afternoon) }}</td>
                    <td>{{ user.firstname }}</td>
                    <td>{{ user.lastname }}</td>

                </tr>
            {% endfor %}
            </tbody>
        </table>
    {% endif %}
